<td class="wid20" Fid="F2" val="<%= getShortVal(data[item]["F2"]) %>" ><%- getF2(data[item]["F2"]) %></td>
<td class="wid90"><div class='btn-group btn-group-xs'><a class="btn btn-default" name="oneupdate" url="/zcar/update?Rid=<%= data[item]["Rid"] %>&format=json&F2=3"><font color="#555555">完成</font></a>

<a class="btn btn-default dropdown-toggle" data-toggle="dropdown">&#20;<span class="caret"></span></a>

<ul class="dropdown-menu" role="menu">
<li><a name="oneupdate" url="/zcar/update?Rid=<%= data[item]["Rid"] %>&format=json&F2=1" class="green"><font color="#008000">实施</font></a>
</li>
<li><a name="oneupdate" url="/zcar/update?Rid=<%= data[item]["Rid"] %>&format=json&F2=2" class="orange"><font color="#ff6600">暂停</font></a>
</li>
<li><a name="oneremove" url="/zcar/update?Rid=<%= data[item]["Rid"] %>&format=json&F2=4" class=""><font color="#000000">关闭</font></a>
</li>
<li><a name="oneupdate" url="/zcar/update?Rid=<%= data[item]["Rid"] %>&format=json&F2=0" class="blue"><font color="#000080">规划</font></a>
</li></ul>
</div>
</td>
<td Fid="F0" val="<%= getShortVal(data[item]["F0"]) %>"  class=""><a  class="" href="/zcar/show?Rid=<%= data[item]["Rid"] %>" title="Click for detail information"><%= ("F0" in data[item]) ? data[item]["F0"] : "" %></a></td>
<td Fid="F6" val="<%= getShortVal(data[item]["F6"]) %>"  class="" width="150px"><%= (data[item]["F6"] || "") %></td>
<td class="" Fid="F7" val="<%= getShortVal(data[item]["F7"]) %>" ><%= (data[item]["F7"] || "") %></td>
<td Fid="F8" val="<%= getShortVal(data[item]["F8"]) %>"  class="" width="90px"><%= (data[item]["F8"] || "") %></td>
<td class="wid30"><a href="/zcar/update?Rid=<%= data[item]["Rid"] %>" title=""><img src="/img/edit.png"></a></td>
<td class="wid30"><a href="/zcar/copy?Rid=<%= data[item]["Rid"] %>" title=""><img src="/img/copy.png"></a></td>
<td class="wid30"><a name="remove" href="javascript:;" url="/zcar/remove?Rid=<%= data[item]["Rid"] %>" name="delete" title=""><img src="/img/remove.png"></a></td>
</tr>

	<% } %>
