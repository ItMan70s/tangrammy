<div class='input-group'><input type='text' name='condition' class='form-control'><span name='search' class='input-group-addon glyphicon glyphicon-search'></span></div>
	</div><% function getF2(val) { 
  var opts = ""; 
  val = "," + (val || "") + ","; 
  if (val.contains( "," + "0" + ",")) {    opts += "<img src='/img/s/b1.png'> "; } 
   if (val.contains( "," + "1" + ",")) {    opts += "<img src='/img/s/b2.png'> "; } 
   if (val.contains( "," + "2" + ",")) {    opts += "<img src=' /img/s/b4.png '> "; } 
   if (val.contains( "," + "3" + ",")) {    opts += "<img src=' /img/s/b7.png '> "; } 
   if (val.contains( "," + "4" + ",")) {    opts += "<img src=' /img/s/b6.png '> "; } 
   return opts; } %>
<div class='input-group hidden'><input type='hidden' class='hidden' name='app' value='zcar'></div><table class='table table-hover'>
