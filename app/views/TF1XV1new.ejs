	<div id="container" class="container" style="padding-top:60px;padding-bottom:60px;">
	
<h1>Add New Users</h1>

<form class='fields-section well col-md-12' method="post" action="/users/new" target="submitFrame" >

<div class='row'>
<div class='col-md-12 <%= (code != 200) ? "alert alert-danger": "" %>'><%= (code != 200) ? message : "" %></div>
<div class='hidden'><input type='hidden' class='hidden' name='format' value='json'>
<input type='hidden' class='hidden' name='app' value='users'>
<input type='hidden' class='hidden' name='Rid' value=''></div><div class='col-md-12'>		<div><label class='control-label title-label'>Name<B class='red'>*</B></label><div class='controls'>
<input type='text' class='form-control' name='F0' value='<%= data["F0"] || "" %>' required=true unique=true placeholder='User name'></div></div>
</div>
<div class='col-md-12'>		<div><label class='control-label title-label'>Email<B class='red'>*</B></label><div class='controls'>
<input type='text' class='form-control' name='F1' value='<%= data["F1"] || "" %>' required=true unique=true placeholder='someone@hp.com'></div></div>
</div>
<div class='col-md-12'>		<div><label class='control-label title-label'>Password<B class='red'>*</B></label><div class='controls'>
<% if (!data["Rid"]) { %> <div class='input-group'><input type='password' class='form-control' name='F2' value='<%= data["F2"] || "" %>' required=true></div><% } else { %> ****** <a href="javascript:changePassword('<%= data["Rid"] || "" %>', 'F2')">Change</a><% } %></div></div>
</div>
<div class='col-md-12'>		<div><label class='control-label title-label'>Role<B class='red'>*</B></label><div class='controls'>
<% var val = data["F3"] || "";  val = "," + val.toString() + ",";%><div class='form-control min-h34'><input type='checkbox' name='F3' value='' checked=true class='hidden' ><div class='min-h20'>
			<label class='checkbox-inline col-md-4 '><input type='checkbox' name='F3' value='Administrator'  <%= (val.contains( "," + "Administrator" + ",")) ? "checked=true" : "" %> >Administrator</label>
			<label class='checkbox-inline col-md-4 '><input type='checkbox' name='F3' value='Default'  <%= (val.contains( "," + "Default" + ",")) ? "checked=true" : "" %> >Default</label></div></div></div></div>
</div>
<div class='col-md-12'>		<div><label class='control-label title-label'>Enable<B class='red'>*</B></label><div class='controls'>
<% var val = data["F4"] || "";  val = "," + val.toString() + ",";%><div class='form-control min-h34'><input type='checkbox' name='F4' value='' checked=true class='hidden' ><div class='min-h20'>
			<label class='checkbox-inline col-md-4 '><input type='checkbox' name='F4' value='1'  <%= (val.contains( "," + "1" + ",")) ? "checked=true" : "" %> >Yes</label>
			<label class='checkbox-inline col-md-4 '><input type='checkbox' name='F4' value='0'  <%= (val.contains( "," + "0" + ",")) ? "checked=true" : "" %> >No</label></div></div></div></div>
</div>
<div class='col-md-12'>		<div><label class='control-label title-label'>TimeZone<B class='red'>*</B></label><div class='controls'>
<% var val = data["F5"] || "";  val = "," + val.toString() + ",";%>
			<select class='form-control' name='F5'  size=3 required=true >
			<option value='-360'  <%= (val.contains( "," + "-360" + ",")) ? "selected=true" : "" %>  >(UTC-06:00) Houston</option>
			<option value='330'  <%= (val.contains( "," + "330" + ",")) ? "selected=true" : "" %>  >(UTC+05:30) India</option>
			<option value='480'  <%= (val.contains( "," + "480" + ",")) ? "selected=true" : "" %>  >(UTC+08:00) China</option>
			</select></div></div>
</div>
<div class='col-md-12'><div class='panel-footer section-action' style='padding-top:20px;'>
<div class="col-md-0"><button type="submit" class="btn btn-primary"    style="min-width:80px;" title="">Save </button></div>
<div class="col-md-0"><a class="btn btn-link"  href="/users/list" style="min-width:80px;" title="">Cancel</a></div>
</div></div>
<span name='history' class='col-md-12'></span>
<script type='text/javascript'>getOPLogs('TF1', 'V1', '<%= data["Rid"] %>');</script></div>
</form>
<iframe id='submitFrame' name='submitFrame' class='hidden'></iframe>

