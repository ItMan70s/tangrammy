<div style="background-color: #FFF; padding-top:60px;"><div id="container" class="container">
	
<h1>Show Roles</h1>

<div class='fields-section well col-md-12'>
<div class='row'>
<div class='input-group hidden'><input type='hidden' class='hidden' name='app' value='roles'></div><div class='col-md-12'><label class='control-label title-label'>Role<B class='red'>*</B></label>
		<div class='form-show' readonly=true><%= ((data["F0"] || '') + '').replace(/(\r\n)|(\r)|(\n)/g, '<br />') %></div></div>
<div class='col-md-12'><label class='control-label title-label'>Application<B class='red'>*</B></label>
<div class='form-show' readonly=true><% var val = data["F1"] || "";  val = "," + val.toString() + ",";%><% if (val.contains( "," + "*" + ",")) { %> <span class='pull-left col-md-auto'>All</span> <% } %><% if (val.contains( "," + "T9/V1" + ",")) { %> <span class='pull-left col-md-auto'>Booking</span> <% } %><% if (val.contains( "," + "T9/V3" + ",")) { %> <span class='pull-left col-md-auto'>Booking3</span> <% } %><% if (val.contains( "," + "T6/V1" + ",")) { %> <span class='pull-left col-md-auto'>Books</span> <% } %><% if (val.contains( "," + "TB/V1" + ",")) { %> <span class='pull-left col-md-auto'>Issues</span> <% } %><% if (val.contains( "," + "T3/V1" + ",")) { %> <span class='pull-left col-md-auto'>Links</span> <% } %><% if (val.contains( "," + "t/V1" + ",")) { %> <span class='pull-left col-md-auto'>Money</span> <% } %><% if (val.contains( "," + "TF0/V1" + ",")) { %> <span class='pull-left col-md-auto'>Roles</span> <% } %><% if (val.contains( "," + "T9/V2" + ",")) { %> <span class='pull-left col-md-auto'>Servers</span> <% } %><% if (val.contains( "," + "T4/V1" + ",")) { %> <span class='pull-left col-md-auto'>Todo</span> <% } %><% if (val.contains( "," + "TF1/V1" + ",")) { %> <span class='pull-left col-md-auto'>Users</span> <% } %><% if (val.contains( "," + "TA/V1" + ",")) { %> <span class='pull-left col-md-auto'>VMs</span> <% } %><% if (val.contains( "," + "T7/V1" + ",")) { %> <span class='pull-left col-md-auto'>Words</span> <% } %><% if (val.contains( "," + "T1/V1" + ",")) { %> <span class='pull-left col-md-auto'>ZCar</span> <% } %><% if (val.contains( "," + "T1/V2" + ",")) { %> <span class='pull-left col-md-auto'>ZCarRelease</span> <% } %><% if (val.contains( "," + "T5/V2" + ",")) { %> <span class='pull-left col-md-auto'>lend</span> <% } %></div></div>
<div class='col-md-12'><label class='control-label title-label'>Enable</label>
<div class='form-show' readonly=true><% var val = data["F2"] || "";  val = "," + val.toString() + ",";%><% if (val.contains( "," + "1" + ",")) { %> <span class='pull-left col-md-auto'>Enable</span> <% } %><% if (val.contains( "," + "0" + ",")) { %> <span class='pull-left col-md-auto'>Disable</span> <% } %></div></div>
<div class='col-md-12'><label class='control-label title-label'>Default Role</label>
<div class='form-show' readonly=true><% var val = data["F3"] || "";  val = "," + val.toString() + ",";%><% if (val.contains( "," + "1" + ",")) { %> <span class='pull-left col-md-auto'>Yes</span> <% } %><% if (val.contains( "," + "0" + ",")) { %> <span class='pull-left col-md-auto'>No</span> <% } %></div></div>
<div class='col-md-12'><div class='panel-footer section-action' style='padding-top:20px;'>
<div class="col-md-0"><a class="btn btn-primary col-md-2"  href="/roles/list" style="min-width:80px;" title="">OK</a></div>
<div class="col-md-0"><a class="btn btn-default"  href="/roles/update?Rid=<%= data["Rid"] %>" style="min-width:80px;" title=""><img src="/img/edit.png">Edit</a></div>
<div class="col-md-0"><a class="btn btn-default"  href="/roles/copy?Rid=<%= data["Rid"] %>" style="min-width:80px;" title=""><img src="/img/copy.png">Copy&Create</a></div>
<div class="col-md-0"><a class="btn btn-default"  href="/roles/new" style="min-width:80px;" title=""><img src="/img/add.png">Add new</a></div>
</div></div>
<span name='history' class='col-md-12'></span>
<script type='text/javascript'>getOPLogs('TF0', 'V1', '<%= data["Rid"] %>');</script>	</div></div>

