	<div id="container" class="container">
<h1>List Users  <small><a  class="" href="/users/new"><img src="/img/add.png" alt="Add User" title="Add User">Add User</a></small></h1>
<form class='navbar-form navbar-left' action='javascrip:doFilter();'>
<div class='form-group small' style='width:230px'><div class='input-group'><input type='text' name='filterstring' class='form-control data-filter' value='<%= (request.u.data["condition"] || "") %>' placeholder='Search in page'>
<div class='input-group-btn'><a class='btn btn-default' filter='' title='Clear search text'>X</a><button type='button' class='btn btn-default dropdown-toggle' data-toggle='dropdown' style='padding-left:5px;padding-right:5px;'>More <span class='caret'></span></button><ul class='dropdown-menu dropdown-menu-right' role='menu'>
	<li><a name='search' href='javascript:;'>Search In Site</a></li>
</ul></div></div></div>
<div class='form-group small'>undefined</div>
	</form><div class='col-md-6 hidden'>
<div class='input-group'><input type='text' name='condition' class='form-control'><span name='search' class='input-group-addon glyphicon glyphicon-search'></span></div>
	</div><table class='table'>
  <thead>
	<tr><div class='input-group hidden'><input type='hidden' class='hidden' name='app' value='users'></div><th  Fid="F0">Name</th><th  Fid="F1">Email</th><th  Fid="F2">Password</th><th  Fid="F3">Role</th><th  Fid="F4">Enable</th><th  Fid="F5">TimeZone</th><th >Edit</th><th >Copy</th>
	</tr>
  </thead>
  <tbody>
	<% if(data.length < 1) { %>
	<tr><td colspan="8">No recorder yet. Add one now? <a  class="" href="/users/new"><img src="/img/add.png" alt="Add User" title="Add User">Add User</a></td></tr>	<% }
 var group = ""; for(var item in data) { 
if (data[item]["F5"] != group) { group = data[item]["F5"]; %> <tr class="group"><th colspan="8"><b class="glyphicon glyphicon-minus-sign"></b> TimeZone: <% var val = data[item]["F5"] || "";  val = "," + val.toString() + ","; %><% if (val.contains( "," + "-360" + ",")) { %><span class='wid150'>(UTC-06:00) Houston<% } %> </span><% if (val.contains( "," + "330" + ",")) { %><span class='wid150'>(UTC+05:30) India<% } %> </span><% if (val.contains( "," + "480" + ",")) { %><span class='wid150'>(UTC+08:00) China<% } %> </span> </th></tr> <% } %>	<tr Rid='<%= data[item]["Rid"] %>'>
  <td Fid="F0" ><%= ("F0" in data[item]) ? data[item]["F0"] : "" %></td>
  <td Fid="F1" ><%= ("F1" in data[item]) ? data[item]["F1"] : "" %></td>
  <td Fid="F2" >******</td>
  <td Fid="F3" ><% var val = data[item]["F3"] || "";  val = "," + val.toString() + ","; %><% if (val.contains( "," + "Administrator" + ",")) { %>Administrator<% } %> <% if (val.contains( "," + "Default" + ",")) { %>Default<% } %> </td>
  <td Fid="F4" ><% var val = data[item]["F4"] || "";  val = "," + val.toString() + ","; %><% if (val.contains( "," + "1" + ",")) { %>Yes<% } %> <% if (val.contains( "," + "0" + ",")) { %>No<% } %> </td>
  <td Fid="F5" ><% var val = data[item]["F5"] || "";  val = "," + val.toString() + ","; %><% if (val.contains( "," + "-360" + ",")) { %><span class='wid150'>(UTC-06:00) Houston<% } %> </span><% if (val.contains( "," + "330" + ",")) { %><span class='wid150'>(UTC+05:30) India<% } %> </span><% if (val.contains( "," + "480" + ",")) { %><span class='wid150'>(UTC+08:00) China<% } %> </span></td>
  <td ><a  class="" href="/users/update?Rid=<%= data[item]["Rid"] %>"><img src="/img/edit.png" alt="" title=""></a></td>
  <td ><a  class="" href="/users/copy?Rid=<%= data[item]["Rid"] %>"><img src="/img/copy.png" alt="" title=""></a></td>

	</tr>
	<% } %>
  </tbody>
</table>

<div class='input-group hidden'><input type='hidden' class='hidden' name='ids' value='F0,F1,'></div><div class='summaryarea'></div><script type='text/javascript'>
  var summaryid = '';

	$("[sub='true']").each(function () {
		var empty = true;
		$(this).find('.spanval').each(function () { if ($(this).html().length > 1) empty = false; });
		if (empty) {this.parentNode.removeChild(this);} else {$(this).removeClass('hidden');}
	});
 summary();</script>


